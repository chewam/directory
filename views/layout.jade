html
    head
        title= title

        link(rel='stylesheet', href='/stylesheets/bootstrap.css')
        link(rel='stylesheet', href='/stylesheets/style.css')

        script(charset="utf-8", src='http://maps.googleapis.com/maps/api/js?sensor=false')
        script(charset="utf-8", src='/javascripts/lib/raphael.js')
        script(charset="utf-8", src='/javascripts/lib/jquery.js')
        script(charset="utf-8", src='/javascripts/lib/modal.js')

        script(charset="utf-8", src='/javascripts/googlemap.js')
        script(charset="utf-8", src='/javascripts/map.js')
        script(charset="utf-8", src='/javascripts/index.js')

        - if (items)
            - var coords = []
            - for (var i = 0, l = items.length; i < l; i++)
                - coords.push('{lat: ' + items[i].get('lat') + ', lng: ' + items[i].get('lng') + '}')

            script
                var coords = [#{coords.join(',')}];

